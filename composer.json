{
    "name": "sniper7kills/survey",
    "description": "Survey package for Laravel",
    "type": "library",
    "require": {
        "illuminate/database": "^6.0|^7.0",
        "illuminate/routing":"^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "ramsey/uuid": "^3.0",
        "spatie/laravel-sluggable": "^2.2",
        "carbondate/carbon": "^1.33"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0",
        "nunomaduro/collision": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sniper7Kills\\Survey\\SurveyServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Sniper7Kills\\Survey\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sniper7Kills\\Survey\\Tests\\": "tests/"
        }
    }
}
